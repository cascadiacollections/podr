{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.153.2",
  "npmVersion": "10.8.2",
  "nodeSupportedVersionRange": ">=18.17.0 <25.0.0",
  "suppressNodeLtsWarning": false,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": true,
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/cascadiacollections/podr",
    "defaultBranch": "main"
  },
  "eventHooks": {},
  "hotfixChangeEnabled": false,
  "variants": [],
  "projects": [
    {
      "packageName": "podr",
      "projectFolder": "apps/podr",
      "reviewCategory": "production",
      "shouldPublish": false
    },
    {
      "packageName": "@cascadiacollections/webpack-api-inliner",
      "projectFolder": "libraries/webpack-api-inliner-plugin",
      "reviewCategory": "production",
      "shouldPublish": true
    }
  ]
}