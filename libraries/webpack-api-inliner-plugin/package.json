{
  "name": "@cascadiacollections/webpack-api-inliner",
  "version": "1.0.0",
  "description": "Webpack plugin to fetch API data at build time and inline it as window variables or save as JSON files",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "heft build --clean",
    "build:ci": "heft build --clean --production",
    "test": "heft test",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "keywords": [
    "webpack",
    "plugin",
    "api",
    "inliner",
    "build-time",
    "inline",
    "static",
    "prerender",
    "performance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cascadiacollections/podr"
  },
  "author": "Cascadia Collections",
  "license": "MIT",
  "devDependencies": {
    "@rushstack/heft": "^0.67.2",
    "@types/node": "22.5.5",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "html-webpack-plugin": ">=5.0.0",
    "webpack": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}